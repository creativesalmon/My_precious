
<div style="margin-top:-20px; padding:0;"/>
    <div class="container ctn">
         <div class="row" style="margin-bottom: 10px;margin-top: 30px;">
            <% t = Team.find(@team.id)%>
            
            <% if t.deadline != nil%>
              <% td = DateTime.now.strftime('%s').to_f %>
              <% cd = t.created_at.strftime('%s').to_f %>
              <% ed = t.deadline.to_datetime.strftime('%s').to_f %>
              <% res = ((td-cd)/(ed-cd)).round(2)*100%>
              <% res = ((td-cd)/(cd)).round(2)*100%>
             <div class="progress" style="height: 30px">
              <div class="progress-bar progress-bar-info progress-bar-striped" role="progressbar" aria-valuenow="<%=res %>" aria-valuemin="0" aria-valuemax="100" style="min-width: 2em;width: <%=res %>%;">
                <p style="margin-top:3px"><%=res.round %>%</p> 
              </div>
            </div>
             <% end %>
          </div>

        <div class="row" >
            <div class="col-md-3" style="right: 5px;">
                <div class="row">
                    <div class="card">
                        <%= render partial: 'home/team/team_info_vm'%>
                    </div>
                </div>
                <div class="row">
                    <div class=" card">
                        <%= render partial: 'home/team/team_menu'%>
                    </div>
                </div>
            </div>
            <%=render partial: 'home/team/team_sche_rend'%>
        </div>
    </div>
    
  
</div>


    